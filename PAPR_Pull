SELECT
    'WLRS' AS MINISTRY_CODE, 
    'Riparian' AS BUSINESS_AREA,
    'RAR' AS SOURCE_SYSTEM_ACRONYM
    A.DEV_ASSESSMENT_ID,
    A.DEV_NATURE_CODE,
    A.REGION_CODE,
    R.DESCRIPTION AS REGION_NAME,
    A.STREAM_CODE,
    S.DESCRIPTION AS STREAM_NAME,
    M.DESCRIPTION AS MUNICIPALITY_NAME,
    A.DEV_CODE,
    B.DESCRIPTION AS DEC_Name,
    A.RIPARIAN_AREA_LENGTH,
    A.PROPOSED_START_DATE,
    A.PROPOSED_END_DATE,
    A.LOCATION_LEGAL_DSC,
    A.LOCATION_STREAM_NAME,
    A.LOCATION_NEW_WATERSHED_CODE,
    A.LOCATION_LATITUDE,
    A.LOCATION_LONGITUDE,
    A.LOT_AREA,
    A.SECTION_9_PART_7_ACTIVITIES_YN,
    A.ALL_PROFESSIONALS_QUALIFIED_YN,
    A.ASSESSMENT_METHOD_FOLLOWED_YN,
    A.CERTIFIED_NO_HADD_YN,
    A.COMPLETE_REPORT_ATTACHED_YN,
    A.REVISION_NUMBER,
    A.WHEN_CREATED,
    A.WHEN_UPDATED,
    A.DEV_AREA,
    A.DFO_AREA_CODE,
    A.RETAIN_ASSESS_REPORT_YN,
    A.DEV_STATUS_CODE,
  
    C.DESCRIPTION AS DEV_Status_Description
FROM
    RAR.RAR_DEV_ASSESSMENTS A
LEFT JOIN
    RAR.RAR_DEV_CDS B ON A.DEV_CODE = B.DEV_CODE
LEFT JOIN
    RAR.RAR_DEV_STATUS_CDS C ON A.DEV_STATUS_CODE = C.DEV_STATUS_CODE
LEFT JOIN
    RAR.RAR_REGION_CDS R ON A.REGION_CODE = R.REGION_CODE
LEFT JOIN
    RAR.RAR_MUNICIPALITIES M ON A.MUNICIPALITY_CODE = M.MUNICIPALITY_CODE
LEFT JOIN
    RAR.RAR_STREAM_TYPE_CDS S ON A.STREAM_CODE = S.STREAM_TYPE_CODE;
